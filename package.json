    "prebuild": "npm run clean",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "*",
    "chalk": "^5.3.0",
    "uuid": "^9.0.0"
